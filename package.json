{
  "name": "@springernature/frontend-package-manager",
  "description": "handles the creation, validation, and publication of packages",
  "version": "4.0.2",
  "license": "LGPL-3.0",
  "keywords": [
    "node",
    "cli",
    "npm"
  ],
  "author": "Alex Kilgour",
  "repository": {
    "type": "git",
    "url": "https://github.com/springernature/frontend-package-manager.git"
  },
  "bin": {
    "sn-package-create": "./bin/create.js",
    "sn-package-validate": "./bin/validate.js",
    "sn-package-publish": "./bin/publish.js"
  },
  "scripts": {
    "test": "jest --colors --verbose --coverage",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@springernature/eslint-config": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^12.1.0",
    "jest": "^24.9.0",
    "mock-fs": "^4.10.2",
    "mock-os": "0.0.1",
    "mock-spawn": "^0.2.6",
    "strip-ansi": "^5.2.0"
  },
  "dependencies": {
    "@springernature/util-cli-reporter": "^1.1.0",
    "@springernature/util-package-extender": "^0.0.4",
    "deepmerge": "^4.2.2",
    "gitignore-globs": "^0.1.1",
    "globby": "^10.0.1",
    "inquirer": "^7.0.0",
    "listr": "^0.14.3",
    "minimatch": "^3.0.4",
    "npm-registry-client": "^8.6.0",
    "pretty-error": "^2.1.1",
    "sanitize-filename": "^1.6.3",
    "semver": "^6.3.0",
    "validate-npm-package-name": "^3.0.0",
    "yargs": "^14.0.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "modulePaths": [
      "<rootDir>/"
    ],
    "testRegex": "__tests__/.*\\.js$",
    "collectCoverageFrom": [
      "**/lib/**/*.js"
    ]
  }
}
