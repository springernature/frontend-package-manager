{
  "name": "@springernature/frontend-package-manager",
  "description": "handles the creation, validation, and publication of packages",
  "version": "0.0.0",
  "license": "LGPL-3.0",
  "keywords": [
    "node",
    "cli",
    "npm"
  ],
  "author": "Alex Kilgour",
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "sn-package-create": "./bin/create.js",
    "sn-package-validate": "./bin/validate.js",
    "sn-package-publish": "./bin/publish.js"
  },
  "scripts": {
    "test": "jest --colors --verbose --coverage --silent",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@springernature/eslint-config": "^2.0.0",
    "eslint": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-unicorn": "^4.0.3",
    "gitignore-globs": "^0.1.1",
    "jest": "^23.1.0",
    "mock-fs": "^4.5.0",
    "nock": "^9.6.0",
    "pmock": "^0.2.3",
    "q": "^1.5.0",
    "rewire": "^3.0.2",
    "strip-ansi": "^4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "inquirer": "^6.1.0",
    "listr": "^0.14.1",
    "minimatch": "^3.0.4",
    "npm-registry-client": "^8.3.0",
    "npm-utils": "^2.0.0",
    "pretty-error": "^2.1.1",
    "sanitize-filename": "^1.6.1",
    "semver": "^5.3.0",
    "validate-npm-package-name": "^3.0.0",
    "yargs": "^12.0.1"
  },
  "jest": {
    "testURL": "http://localhost/",
    "modulePaths": [
      "<rootDir>/"
    ],
    "testRegex": "__tests__/.*\\.js$",
    "collectCoverageFrom": [
      "**/lib/**/*.js"
    ]
  }
}
